.coustomFormLyout{
    :global{
        .ant-form-item-control-wrapper{
            flex: 1 1 0%;
        }
        .ant-form-item{display:flex;}
        .ant-calendar-picker{width:100%}
        button{margin:0 4px}
    }
}
:global{
    .ant-menu-sub .ant-menu-item{
        padding-left:26px!important
    }   
    .ant-upload-list-item{
        background:#e4f3f9
    }
    .ant-table-thead > tr > th, .ant-table-tbody > tr > td{
        padding: 10px 10px!important;
    }
    .nestedTable .ant-table-expanded-row > td:last-child {
        padding: 0 48px 0 8px;
    }  
    .nestedTable .ant-table-expanded-row > td:last-child .ant-table-thead th {
        border-bottom: 1px solid #e9e9e9;
    }  
    .nestedTable .ant-table-expanded-row > td:last-child .ant-table-thead th:first-child {
        padding-left: 0;
    }  
    .nestedTable .ant-table-expanded-row > td:last-child .ant-table-row td:first-child {
        padding-left: 0;
    }  
    .nestedTable .ant-table-expanded-row .ant-table-row:last-child td {
        border: none;
    }  
    .nestedTable .ant-table-expanded-row .ant-table-thead > tr > th {
        background: none;
    }  
    .nestedTable .table-operation a:not(:last-child) {
        margin-right: 24px;
    }  
    .nestedTable .ant-table-expanded-row:hover > td {
        background: #fbfbfb;
    }
    .exportBtnInline{display:inline-block!important}
    .exportBtnInline .ant-upload-list{display: inline-block;}
    .exportBtnInline .ant-upload-list-item{display: inline-block;}
    .ant-layout{min-height:100vh}
}
